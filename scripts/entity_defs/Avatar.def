<root>
	<Volatile>
		<position/>
		<!--<position> 0 </position> Don't update-->
		<yaw/>
		<!--<pitch> 20 </pitch>-->
		<pitch/>
		<roll/>
	</Volatile>

	<Implements>
		<Interface>	GameObject		</Interface>
		<Interface>	State			</Interface>
		<Interface>	Motion			</Interface>
		<Interface>	SkillBox		</Interface>
		<Interface>	Combat			</Interface>
		<Interface>	Spell			</Interface>
		<Interface>	Teleport		</Interface>
	</Implements>

	<Properties>
		<roleType>
			<Type> UINT8 </Type>
			<Flags> BASE </Flags>
			<Persistent> true </Persistent>
		</roleType>
		<level>
			<Type> UINT16 </Type>
			<Flags> CELL_PUBLIC_AND_OWN </Flags>
			<Persistent> true </Persistent>
		</level>
		<itemList>
			<Type> ITEM_INFO_LIST </Type>
			<Flags> BASE_AND_CLINET </Flags>
			<Persistent> true </Persistent>
		</itemList>
	</Properties>

	<ClientMethods>
        <ReceiveChatMessage>
        	<Arg>	UNICODE	</Arg>
        </ReceiveChatMessage>

        <onReqItemList>
        	<Arg>   ITEM_INFO_LIST   </Arg>
        </onReqItemList>

	</ClientMethods>

	<BaseMethods>
		<createCell>
			<Arg>		MAILBOX		</Arg>
		</createCell>
		<sendChatMessage>
        	<Arg>		UNICODE		</Arg>
        	<Exposed/>
        </sendChatMessage>
        <reqItemList>
        	<Exposed/>
        </reqItemList>
        <addItem>
        	<Arg>  
        	<Exposed/>
        </addItem>

	</BaseMethods>

	<CellMethods>
		<relive>
			<Exposed/>
			<Arg>		UINT8		</Arg>
		</relive>
	</CellMethods>

</root>
